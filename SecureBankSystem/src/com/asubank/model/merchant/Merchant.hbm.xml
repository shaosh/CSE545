<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.asubank.model.merchant">
	<class name="Merchant" table="merchant">
			<id name="Id" column="id">
			<generator class="increment" />
		</id>
		<property name="checkingID">
		    <column name="checkingid" length="12" not-null="true" />
		</property>
		<property name="customerid">
		    <column name="customerid" length="12" not-null="true" />
		</property>
		<property name="pay_amount">
		    <column name="pay_amount" length="12" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>